<div class="container" style="background-image: url('login/images/prof.jpg');">
<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <center>
        <h1 style="color: white">User Profile</h1>
        <hr>
        <h2 style="color: white">My Orders</h2>
        </center>
        {{# each orders }}
        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{# each this.items }}
                    <li class="list-group-item">
                        <span class="badge">{{ this.price }}</span>
                        {{ this.item.title }} <==> {{ this.qty }} Units
                    </li>
                    {{/each}}
                </ul>
            </div>
            <div class="panel-footer">
                <strong>Total Price: INR {{ this.cart.totalPrice }}</strong>
            </div>
        </div>
        {{/each}}
    </div>
</div>
</div>